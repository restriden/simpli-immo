{
  "expo": {
    "name": "simpli.immo",
    "slug": "simpli-immo",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "simpli-immo",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.simpli.immo"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F97316"
      },
      "package": "com.simpli.immo"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}